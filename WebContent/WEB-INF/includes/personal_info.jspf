<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<div class="personal_info">
	<p><fmt:message key="label.first.name"/>:<c:out value="${sessionScope.currentUser.firstName}"/> </p>
	<p><fmt:message key="label.last.name"/>: <c:out value="${sessionScope.currentUser.lastName}"/></p>
	<p><fmt:message key="label.email"/>: <c:out value="${sessionScope.currentUser.email}"/></p>
</div>